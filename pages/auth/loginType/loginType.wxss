.xttxt{
  width: 570rpx;
  margin: 0 auto 60rpx auto;
  line-height: 36rpx;
  text-align: justify;
  font-size: 22rpx;
  color: #868686;
}
.alert{
	display: flex;
	align-items: center;
	justify-content: center;
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
}
.alert .bg{
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background-color: rgba(0,0,0,.6);
	z-index: 2;
}
.alert .con{
	position: relative;
	z-index: 3;
	background-color: white;
	width: 80%;
	border-radius: 10px;
	overflow: hidden;
}
.alert .con .title{
	padding: 30rpx;
	text-align: justify;
}
.alert .set-k{
	display: flex;
	align-items: center;
	margin-top: 20rpx;
	border-top: 1px solid #F4F4F4;
	position: relative;
}
.alert .bb{
	flex-grow: 1;
	height: 90rpx;
	text-align: center;
	line-height: 90rpx;
	border: none;
	background-color: white;
	font-size: 36rpx;
}
.alert .bb.exit{
	border-right: 1px solid #F4F4F4;
	color: #797979;
}
.alert .bb.wxs:after{
	display: none;
}
.alert .bb.wxs{
	color: #23459b;
}
.agreement{
  width: 520rpx;
  height: 100rpx;
  /* background: pink; */
  margin: 0 auto;
  font-size: 26rpx;
  position: relative;
}
.agreement span{
  padding-left: 10rpx;
}
.agreement text{
  color:#23459b;
}
