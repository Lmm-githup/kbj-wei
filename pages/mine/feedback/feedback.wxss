page{
  background-color: #eef0f3;
  height: 100%;
}
.m .head{
  height: 76rpx;
  padding: 0 78rpx;
  background: url(https://app.fjxzj.com/weimob/ic-phone-q.png) no-repeat;
  background-size: 38rpx 38rpx;
  background-position: 30rpx center;
  font-size: 26rpx;
  color: #333333;
  line-height: 76rpx;
  background-color: #f0ddc4;
  position: relative;
}
.m .head .icon{
  color: #8d8d8d;
  padding-right: 30rpx;
  height: 76rpx;
  line-height: 76rpx;
  background: url(https://app.fjxzj.com/weimob/login_ic_ret.png) no-repeat right center;
  background-size: 15rpx 25rpx;
  font-size: 26rpx;
  position: absolute;
  right: 18rpx;
  top: 0;
}
.forms{
  margin-top: 20rpx;
}
.block{
  background-color: white;
  padding: 30rpx 22rpx;
  margin-bottom: 20rpx;
}
.block .tit{
  font-size: 30rpx;
  color: #333333;
  margin-left: 10rpx;
}
.block .tit .s{
  display: inline;
  color: #868686;
  font-size: 26rpx;
}
.radio-list{
  display: flex;
  flex-wrap: wrap;
  margin-top: 32rpx;
  width: 770rpx;
}
.radio-list .ra{
  margin-right: 53rpx;
  width: 200rpx;
  height: 56rpx;
  position: relative;
  border: 1px solid #868686;
  color: #868686;
  line-height: 56rpx;
  text-align: center;
  box-sizing: border-box;
  font-size: 26rpx;
  border-radius: 4rpx;
  margin-bottom: 29rpx;
}
.radio-list .ra.action{
  border: 1px solid #f69106;
  color: #f69106;
  background-color: #fde9cd;
}
.comm{
  width: 706rpx;
  height: 250rpx;
  background-color: #e4e4e4;
  margin: 30rpx auto;
  position: relative;
}
.comm textarea{
  width: 100%;
  height: 100%;
  border: none;
  font-size: 26rpx;
  padding: 26rpx;
  box-sizing: border-box;
}
.comm .num{
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  font-size: 22rpx;
  color: #868686;
}
.block.nopadding{
  padding: 0;
}
.phone{
  height: 90rpx;
  margin: 0 30rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #333333;
}
.but{
  margin: 50rpx auto 20rpx auto;
  width: 650rpx;
  height: 75rpx;
  border-radius: 6rpx;
  line-height: 75rpx;
  text-align: center;
  color: #ffffff;
  background-color: #24459a;
  font-size: 28rpx;
}
.ts{
  text-align: center;
  font-size: 24rpx;
  color: #36a3ff;
}
.ts .ss{
  display: inline;
  text-decoration: underline;
}
.txttxt{
  font-size: 26rpx;
  color: rgb(87, 87, 87);
}