.list{
  height:130rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f5f5f5;
}
.list .left{
  margin-left: 30rpx;
}
.list .title{
  font-size: 30rpx;
  margin-bottom: 13rpx;
  color: #383838;
}
.list .else{
  font-size: 23rpx;
  color: #5c5c5c
}
.list .right{
  font-size: 30rpx;
  margin-right: 30rpx;
  color: #21469b;
}