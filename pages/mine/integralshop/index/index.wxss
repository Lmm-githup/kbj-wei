page{
  background-color: #eef0f3;
}
.header{
  background-color: #eef0f3;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
}
.header .info{
  height: 120rpx;
  display: flex;
  align-items: center;
  background: -webkit-linear-gradient(left,#fec303,#f49716);
  color: #ffffff;
  position: relative;
  margin-bottom: 16rpx;
}
.header .info .h{
  margin-right: 23rpx;
  width: 70rpx;
  height: 70rpx;
  margin-left: 24rpx;
  background-color: aqua;
}
.header .info .in .name{
  font-size: 26rpx;
  line-height: 1;
  margin-bottom: 12rpx;
}
.header .info .in .phone{
  font-size: 24rpx;
  line-height: 1;
}
.header .info .jf{
  height: 40rpx;
  position: absolute;
  top: 50%;
  margin-top: -20rpx;
  line-height: 40rpx;
  font-size: 32rpx;
  right: 30rpx;
}
.header .nav{
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #ffffff;
  border-top: 1px solid #d5d5d5;
  border-bottom: 1px solid #d5d5d5;
}
.header .nav .na{
  font-size: 28rpx;
  color: #505050;
}
.header .nav .na.action{
  color: #f49716;
}
.header .nav .na:nth-child(1){
  margin-left: 37rpx;
}
.header .nav .na:nth-last-child(1){
  margin-right: 37rpx;
}
.con .zw{
  height: 233rpx;
}
.con-con{
  display: flex;
  padding: 0 33rpx;
  justify-content: space-between;
  flex-wrap: wrap;
}
.con-con .list{
  width: 330rpx;
  height: 400rpx;
  border: 1px solid #dcdcdc;
  background-color: #ffffff;
  margin-bottom: 16rpx;
  border-radius: 6rpx;
}
.con-con .list .t{
  width: 224rpx;
  height: 188rpx;
  margin: 40rpx auto 72rpx auto;
}
.con-con .list .title{
  margin: 0 42rpx;
  line-height: 1;
  font-size: 30rpx;
  color: #505050;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.con-con .list .int{
  margin-top: 8rpx;
  line-height: 1;
  font-size: 28rpx;
  color: #f49716;
  margin: 10rpx 42rpx 0 42rpx;
}
.con-con .list .xj{
  width: 168rpx;
  height: 170rpx;
  background: url(https://app.fjxzj.com/weimob/list-xj.png);
  background-size: 100% 100%;
  margin: 73rpx auto 57rpx auto;
  position: relative;
}
.con-con .list .xj .price{
  width: 100%;
  text-align: center;
  position: absolute;
  left: 0;
  top: 26rpx;
}
.con-con .list .xj .price view{
  display: inline;
  color: #ffffff;
}
.con-con .list .xj .price view.ic{
  font-size: 30rpx;
}
.con-con .list .xj .price view.num{
  font-size: 46rpx;
}