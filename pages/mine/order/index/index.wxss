page{
  background-color: #eef0f3;
}
.k{
  padding-top: 16rpx;
}
.list{
  border-top: 1px solid #dddddd;
  border-bottom: 1px solid #dddddd;
  background-color:#fff;
  padding-bottom:10rpx;
  margin-bottom: 28rpx;
}
.list .top{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 122rpx;
  border-bottom: 1px dashed #e3e3e3;
}
.list .top .left{
  margin-left: 29rpx;
}
.list .top .left .tt{
  font-size: 26rpx;
  color: #333333;
  line-height: 1;
  margin-bottom: 27rpx;
}
.list .top .left .no{
  font-size: 26rpx;
  color: #868686;
  line-height: 1;
}
.list .top .right{
  margin-right: 19rpx;
  overflow: hidden;
}
.list .top .right .time{
  line-height: 1;
  font-size: 24rpx;
  color: #868686;
  clear: both;
}
.list .top .right .bt{
  width: 100rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  border-radius: 18rpx;
  border: 1px solid #1e479c;
  color: #1e479c;
  font-size: 24rpx;
  margin-top: 20rpx;
  float: right;
}
.list .top .right .bx{
  margin-top: 33rpx;
  color: #a6a6a6;
  font-size: 24rpx;
  text-align: right;
}
.list .bottom{
  margin-top: 20rpx;
  margin-left: 29rpx;
}
.list .bottom .ll{
  font-size: 26rpx;
  line-height: 1;
  margin-bottom: 23rpx;
  color: #868686;
}
.list .bottom .ll .ts{
  color: #ff6600;
  display: inline;
}
.bottom-bg{
  position: fixed;
  top:0;
  left:0;
  width: 100%;
  background-color: rgba(0,0,0,0.5);
  display: none;
}
.bottom-bg.action{
  display:block;
}
.bottom-set{
  position: fixed;
  bottom: -1000rpx;
  width: 100%;
  height: 900rpx;
  background-color: #fff;
  font-size: 30rpx;
  color:#333333;
  z-index: 112;
  transition: all 0.3s;
}
.bottom-set.action{
  bottom: 0;
}
.bottom-set .title{
  text-align: center;
  margin: 42rpx auto 60rpx auto;
}
.bottom-set .t-t{
  margin-left: 42rpx;
  margin-bottom: 20rpx;
}
.bottom-set .ypf{
  margin-left: 42rpx;
  position: relative;
  height: 46rpx;
  width: 367rpx;
  margin-bottom: 10rpx;
}
.bottom-set .tit{
  height: 100%;
  line-height: 46rpx;
  top: 0;
  left: 0;
  position: absolute;
}
.bottom-set .ylk,.bottom-set .ac{
  height: 34rpx;
  position: absolute;
  left: 150rpx;
  top: 50%;
  margin-top: -17rpx;
}
.bottom-set .ylk image,.bottom-set .ac image{
  width: 34rpx;
  height: 34rpx;
  float: left;
  margin-right: 6rpx;
}
.pj-bottom{
  margin: 37rpx 30rpx 50rpx 30rpx;
  padding-top: 40rpx;
  border-top: 1px solid #a6a6a6;
}
.pj-bottom .tt-tt{
  font-size: 28rpx;
  color: #333333;
  margin-left: 10rpx;
}
.pj-bottom .linetxt{
  height: 316rpx;
  background-color: #eef0f3;
  width: 100%;
  border-radius: 4rpx;
  margin-top: 16rpx;
  box-sizing: border-box;
  padding: 10rpx;
  margin-bottom: 50rpx;
}
.pj-bottom .btn{
  width: 260rpx;
  height: 70rpx;
  border-radius: 8rpx;
  background-color: #858585;
  text-align: center;
  line-height: 70rpx;
  color: #ffffff;
  font-size: 28rpx;
}
.pj-bottom .btn.esc{
  float: left;
  margin-left: 36rpx;
}
.pj-bottom .btn.sub{
  float: right;
  margin-right: 36rpx;
  background-color: #0045a2;
}

.tips{
  position: fixed;
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 26rpx;
  top: 50%;
  margin-top: -15rpx;
  left: 0;
}