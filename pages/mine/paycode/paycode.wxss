.pay-con{
  width: 650rpx;
  background-color:white;
  padding: 50rpx 0 60rpx 0;
  margin: auto;
  position: relative;
  margin-top: 27rpx;
}
.pay-con .image-k{
  position: relative;
  width: 630rpx;
  height: 450rpx;
  margin: auto;
  transform-style: preserve-3d;
  transition: transform 1s;
}
.pay-con .sks{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  transition: transform 0.5s;
  -webkit-transition: transform 0.5s;
  transform: scale(0);
  -webkit-transform: scale(0);
}
.pay-con .ac .sks{
  transform: scale(1);
  -webkit-transform: scale(1);
  z-index: 3;
}
.pay-con .txm{
  display: block;
  width: 100%;
  height: 170rpx;
