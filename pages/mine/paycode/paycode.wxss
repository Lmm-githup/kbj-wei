  margin: auto;
}
.pay-con .er-k{
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #fff;
  top: 0;
  left: 0;
  transition: transform 0.5s;
  -webkit-transition: transform 0.5s;
  transform: scale(1);
  -webkit-transform: scale(1);
  z-index: 2;
}
.pay-con .ac .er-k{
  transform: scale(0);
  -webkit-transform: scale(0);
}
